{
	"name": "geniemouse-eleventy-starter",
	"description": "GenieMouse Eleventy starter project codebase",
	"author": "Lucy Barker, GenieMouse Ltd",
	"keywords": ["11ty", "bun", "eleventy", "esm", "geniemouse", "static site"],
	"language": "en",
	"locale": "en-GB",
	"main": ".eleventy.js",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"build": "bunx @11ty/eleventy",
		"clean": "rimraf --glob _site **/*.map",
		"preview": "bunx vite preview --outDir _site --port 8080",
		"serve": "bunx @11ty/eleventy --serve --incremental"
	},
	"devDependencies": {
		"@11ty/eleventy": "^3.0.1-alpha.4",
		"@11ty/eleventy-plugin-vite": "^6.0.0-alpha.4",
		"@biomejs/biome": "^1.9.4",
		"@dotenvx/dotenvx": "^1.33.0",
		"browserslist": "^4.24.4",
		"html-minifier-terser": "^7.2.0",
		"js-beautify": "^1.15.1",
		"lightningcss": "^1.29.1",
		"rimraf": "^6.0.1",
		"rollup-plugin-critical": "^1.0.14",
		"vite": "^6.0.11"
	},
	"browserslist": ["> 0.25%", "not op_mini all", "not dead"]
}
