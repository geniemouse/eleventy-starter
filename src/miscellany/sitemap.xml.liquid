---
eleventyExcludeFromCollections: true
excludeFromSitemap: true
layout: false
permalink: /public/sitemap.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for page in collections.siteMapOrder %}
	{%- assign lastModified = page.data.last_updated | fallback: page.date %}
	<url>
		<loc>{{ page.url | absoluteUrl: app.site.url }}</loc>
		<lastmod>{{ lastModified | toISODateTime }}</lastmod>
	</url>
{% endfor %}
</urlset>
